{
  "name": "ts-node-boiler",
  "version": "1.0.0",
  "description": "typescript nodejs boilerplate",
  "main": "bin/www",
  "scripts": {
    "install": "npx husky install",
    "start": "node build/src/index.js",
    "dev": "nodemon build/src/index.js",
    "compile": "tsc -p tsconfig.json",
    "build": "npm run compile && npm run start",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "author": "Kartik",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/async": "^3.2.8",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/nconf": "^0.10.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "git-cz": "^4.8.0",
    "husky": "^7.0.2",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "async": "^3.2.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nconf": "^0.11.3",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git"
  },
  "_moduleAliases": {
    "@lib": "build/src/lib",
    "@routes": "build/src/routes",
    "@root": "build/src",
    "@app": "build/src/app"
  }
}
